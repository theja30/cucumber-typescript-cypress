  },
  "homepage": "https://github.com/theja30/cucumber-typescript-cypress#readme",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^23.0.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.8",
    "concurrently": "^9.2.1",
    "cypress": "^15.10.0",
    "cypress-parallel": "^0.15.0",
    "cypress-split": "^1.24.28",
    "esbuild": "^0.27.3",
    "multiple-cucumber-html-reporter": "^3.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
