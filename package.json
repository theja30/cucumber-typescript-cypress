{
  "name": "cucumber-typescript-cypress",
  "version": "1.0.0",
  "description": "Automation Framework using Cucumber Typescript and Cypress",
  "main": "index.js",
  "scripts": {
    "cy:run": "cypress run --browser chrome",
    "test:parallel": "npm run clean:reports && cypress-parallel -s cy:run -t 4 -d \"cypress/e2e/**/*.feature\" && npm run report",
    "report": "ts-node generate-report.ts",
    "clean:reports": "rm -rf reports && mkdir -p reports"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theja30/cucumber-typescript-cypress.git"
  },
