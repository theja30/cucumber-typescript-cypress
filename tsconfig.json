{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6", "dom"],
    "types": ["cypress", "node"],
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "strict": false,
    "baseUrl": "./",
    "paths": {
      "@pages/*": ["cypress/pages/*"]
    },
     "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "include": ["cypress/**/*.ts"]
}
